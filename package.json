		"protractor": "~7.0.0",
		"semantic-release": "^18.0.0",
		"ts-node": "~8.3.0",
		"tslint": "~6.1.0",
		"typescript": "~4.1.2"
	},
	"release": {
		"pkgRoot": "dist/ngx-dom-navigator"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
